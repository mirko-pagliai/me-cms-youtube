{
    "name": "mirko-pagliai/me-cms-youtube",
    "description": "Youtube plugin for MeCms",
    "homepage": "https://github.com/mirko-pagliai/me-cms-youtube",
    "type": "cakephp-plugin",
    "license": "AGPL",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "config": {
        "component-dir": "vendor/components"
    },
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.4 <3.4",
        "mirko-pagliai/me-cms": ">=2.15.0 <3"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "2.*",
        "mirko-pagliai/reflection": "*"
    },
    "autoload": {
        "psr-4": {
            "MeCmsYoutube\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCmsYoutube\\Test\\": "tests",
            "MeCms\\Test\\": "vendor/mirko-pagliai/me-cms/tests",
            "App\\": "tests/test_app/TestApp"
        }
    }
}
